# VittoriaDB RAG Web UI - Development Environment Configuration

# OpenAI API Key (required for AI responses)
OPENAI_API_KEY=your_openai_api_key_here

# GitHub Token (optional, for private repos and higher rate limits)
GITHUB_TOKEN=your_github_token_here

# Service URLs (development values)
VITTORIADB_URL=http://vittoriadb:8080
OLLAMA_URL=http://ollama:11434
REDIS_URL=redis://redis:6379

# Frontend URLs
NEXT_PUBLIC_API_URL=http://localhost:8501
NEXT_PUBLIC_WS_URL=ws://localhost:8501

# Development settings
NODE_ENV=development
ENVIRONMENT=development
LOG_LEVEL=DEBUG
NEXT_TELEMETRY_DISABLED=1

# Development features
WATCHPACK_POLLING=true
CHOKIDAR_USEPOLLING=true
OLLAMA_DEBUG=1
VITTORIADB_LOG_LEVEL=DEBUG

# Hot reload settings
FAST_REFRESH=true
REACT_STRICT_MODE=true
